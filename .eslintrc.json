{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "react/react-in-jsx-scope": "off", 
    "next/no-img-element": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "no-var": "off"  
  },
  "overrides": [
    {
      "files": ["*.js", "*.jsx", "*.ts", "*.tsx"],
      "rules": {
        "no-unused-vars": "off" 
      }
    }
  ]
}
